<UserControl x:Class="RenderApp.View.Controller.VoxelView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:RenderApp.ViewModel"
        xmlns:dvm="clr-namespace:RenderApp.ViewModel.Dialog"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Height="180" Width="240">
    <StackPanel>
        <TextBlock Text="分割数" Width="80" Height="20" FontSize="16" TextAlignment="Center"/>
        <TextBox Text="{Binding PartitionNum}" Width="80" Height="20"/>
        <Button Content="Execute" Width="80" Height="40" Command="{Binding Execute}"/>
    </StackPanel>
</UserControl>
