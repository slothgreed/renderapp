<UserControl x:Class="RenderApp.View.Controller.VoxelView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:RenderApp.ViewModel"
        xmlns:dvm="clr-namespace:RenderApp.ViewModel.Dialog"
        xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <WrapPanel>
        <TextBlock Text="分割数" FontSize="16" Margin="2" TextAlignment="Center"/>
        <TextBox Text="{Binding PartitionNum}" Width="80"/>
        <Button Content="Execute" Width="80" Command="{Binding Execute}" Margin="2"/>
    </WrapPanel>
</UserControl>
