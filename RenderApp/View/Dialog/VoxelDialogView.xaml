<Window x:Class="RenderApp.View.Dialog.VoxelDialogView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:RenderApp.ViewModel"
        xmlns:dvm="clr-namespace:RenderApp.ViewModel.Dialog"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        Title="VoxelDialogView" Height="180" Width="240">

    <Window.DataContext>
        <dvm:VoxelDialogViewModel/>
    </Window.DataContext>
    <Grid>
        <TextBlock Text="分割数" Width="80" Height="20" Margin="24,10,111,73" FontSize="16" TextAlignment="Center"/>
        <TextBox Text="{Binding PartitionNum}" Width="80" Height="20" Margin="116,10,19,73"/>
        <Button Content="OK" Width="80" Height="40" Margin="24,73,111,10" Click="OK_Click"/>
        <Button Content="Cancel" Command="{Binding Close}" Width="80" Height="40" Margin="116,73,19,10" Click="Cancel_Click"/>
        
    </Grid>
</Window>
